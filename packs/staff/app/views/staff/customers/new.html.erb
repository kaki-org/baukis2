<% @title = '顧客の新規登録' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @customer_form, scope: 'form', url: :staff_customers do |fv| %>
    <%= render 'form', f: fv %>
    <div class="buttons">
      <%= fv.submit '登録' %>
      <%= link_to 'キャンセル', :staff_customers %>
    </div>
  <% end %>
</div>
