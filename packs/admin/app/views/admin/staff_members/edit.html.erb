<% @title = '職員アカウントの編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @staff_member, url: [:admin, @staff_member] do |fv| %>
  <%= render 'form', f: fv %>
    <div class="buttons">
      <%= fv.submit '更新' %>
      <%= link_to 'キャンセル', :admin_staff_members %>
    </div>
  <% end %>
</div>
